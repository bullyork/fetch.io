{
  "name": "fetch.io",
  "version": "0.1.0",
  "description": "extends the whatwg fetch api, makes it easier to use",
  "main": "index.js",
  "scripts": {
    "test": "mocha -R spec -t 5000 test/*.js",
    "test-cov": "istanbul cover node_modules/.bin/_mocha -- -R dot -t 5000 test/*.js",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- -R dot -t 5000 test/*.js",
    "test-browser": "npm run build-test && node_modules/karma/bin/karma start test-browser/karma.conf.js",
    "build-test": "webpack test/index.js --output-file test-browser/index.js"
  },
  "repository": "haoxins/fetch.io",
  "keywords": [
    "extend",
    "fetch"
  ],
  "files": [
    "index.js",
    "lib/"
  ],
  "author": "haoxin",
  "license": "MIT",
  "devDependencies": {
    "istanbul-harmony": "0",
    "jasmine-core": "2",
    "karma": "0",
    "karma-chrome-launcher": "0",
    "karma-coverage": "0",
    "karma-jasmine": "0",
    "mocha": "2",
    "node-libs-browser": "0",
    "webpack": "1"
  },
  "dependencies": {
    "form-data": "^0.2.0",
    "node-fetch": "^1.3.1"
  }
}
